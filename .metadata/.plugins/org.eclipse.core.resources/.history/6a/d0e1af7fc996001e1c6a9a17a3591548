#include "stm32f407xx.h"
#include "stm32f407_i2c_driver.h"
#include "stm2f407_gpio_driver.h"


void I2C_PeriClockControl(I2C_RegDef_t *pI2Cx, uint8_t EnorDi){
	if(EnorDi == ENABLE){
		if(pI2C ==  I2C1)
			I2C1_PCLK_EN();
		if(pI2C ==  I2C1)
					I2C1_PCLK_EN();
		if(pI2C ==  I2C1)
					I2C1_PCLK_EN();

	}
}
